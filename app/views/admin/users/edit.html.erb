<div class="col-sm-12">
	<h2>お問い合わせ<h2>
	<%= form_with model: @user, local: true, url: admin_user_path do |f| %>
		<div class="form-group">
			<%= f.label :名前 %><br>
			<%= f.text_field :name %>
		</div>
		<div class="form-group">
			<%= f.label :画像 %><br>
			<%= f.attachment_field :image %>
		</div>
		<div class="form-group">
			<%= f.label :プロフィール文 %><br>
			<%= f.text_area :profile %>
		</div>
		<div class="form-group"><br>
			<%= f.submit "更新" %>
		</div>
	<% end %>
</div>

